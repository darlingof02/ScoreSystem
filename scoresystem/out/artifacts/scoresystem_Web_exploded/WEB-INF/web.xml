<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>example</servlet-name>
        <servlet-class>test.example</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Studentlogin</servlet-name>
        <servlet-class>test.Student.Studentlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teacherlogin</servlet-name>
        <servlet-class>test.Teacher.Teacherlogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>example</servlet-name>
        <url-pattern>/Mainpage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Studentlogin</servlet-name>
        <url-pattern>/Studentlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teacherlogin</servlet-name>
        <url-pattern>/Teacherlogin</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>/studentlogin.jsp</welcome-file>
    </welcome-file-list>
</web-app>

